@namespace MpStream.Pages.Member.Movies
<h1 class="bg-yellow-800 rounded py-1 px-2 text-gray-300">ภาษา</h1>
<div class="text-sm bg-gray-800 p-2 mt-2 rounded">
    @foreach (var sound in MovieSoundList)
    {
        <a href="/movie/sound/@sound" class="w-1/5 bg-gray-700 m-1 rounded text-gray-400 hover:bg-gray-800 transition duration-500 ease-in-out">
            <div class="">@sound</div>
        </a>
    }
</div>
